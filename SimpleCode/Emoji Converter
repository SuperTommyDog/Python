message = input(">")
words = message.split(" ")
emojis = {
    ":)": "😊",
    "<3": "❤",
    ":(": "☹",
    ":/": "😐",
    ":P": "😛",
    ":'(": "😥",
    ":D": "😀",
    ":O": "😮",
    ";P": "😜",
    ">:(": "'😤"
}
output = ""
for word in words:
    output += emojis.get(word, word) + " "
print(output)
